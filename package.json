{
  "name": "react-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate setup with parcel for a React project.",
  "private": true,
  "scripts": {
    "start": "parcel public/index.html -p 3000 --open",
    "build": "parcel build public/index.html --no-source-maps",
    "format": "prettier --check \"**/*.{js,jsx,json,md,yml,html}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,json,md,yml,html}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jongwooo/react-parcel-boilerplate.git"
  },
  "author": "jongwooo <han980817@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jongwooo/react-parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/jongwooo/react-parcel-boilerplate#readme",
  "packageManager": "yarn@3.2.4",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.7.0",
    "@parcel/transformer-svg-react": "^2.7.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  }
}
